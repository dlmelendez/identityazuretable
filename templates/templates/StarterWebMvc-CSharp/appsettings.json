{
  "IdentityAzureTable": {
    "IdentityConfiguration": {
      "TablePrefix": "v7",
      "StorageConnectionString": "UseDevelopmentStorage=true;",
      "IndexTableName": "AspNetIndex",
      "RoleTableName": "AspNetRoles",
      "UserTableName": "AspNetUsers"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}